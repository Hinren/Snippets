<Snippet>
    <Declarations>
      <Literal>
        <ID>identifier</ID>
        <ToolTip>Identifier name</ToolTip>
        <Default>s</Default>
      </Literal>
      <Literal>
        <ID>value</ID>
        <ToolTip>Value to assign</ToolTip>
        <Default>null</Default>
      </Literal>
    </Declarations>
    <Code Language="CSharp"><![CDATA[string $identifier$ = $value$;$end$]]></Code>
  </Snippet>
