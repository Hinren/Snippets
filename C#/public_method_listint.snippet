        <Snippet>
            <Declarations>
                <Literal>
                    <ID>ReturnList</ID>
                    <ToolTip>Change method name</ToolTip>
                    <Default>ReturnList</Default>
                </Literal>
                <Literal>
                    <ID>numberCollection</ID>
                    <ToolTip>Change collection name</ToolTip>
                    <Default>numberCollection</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp">
              
<![CDATA[public List<int> $ReturnList$()
                 {
                     List<int> $numberCollection$ = new List<int>();

                     $end$

                     return numberCollection;
                 }]]>	
              
            </Code>
          <Imports>
