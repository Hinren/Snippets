<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>Constructor with 4 parameters</Title>
      <Author>Patryk Wi≈õniewski</Author>
      <Shortcut>ctor4p</Shortcut>
      <Description>Code snippet to generate a constructor with 4 parameters and backing fields.</Description>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="false">
          <ID>classname</ID>
          <ToolTip>Class name</ToolTip>
          <Function>ClassName()</Function>
          <Default>ClassNamePlaceholder</Default>
        </Literal>
        <Literal>
          <ID>firstParameter</ID>
          <ToolTip>Replace with the name of the first parameter</ToolTip>
          <Default>firstParameter</Default>
        </Literal>
        <Literal>
          <ID>firstType</ID>
          <ToolTip>Replace with the name of the first type</ToolTip>
          <Default>firstType</Default>
        </Literal>
        <Literal>
          <ID>secondParameter</ID>
          <ToolTip>Replace with the name of the second parameter</ToolTip>
          <Default>secondParameter</Default>
        </Literal>
        <Literal>
          <ID>secondType</ID>
          <ToolTip>Replace with the name of the second type</ToolTip>
          <Default>secondType</Default>
        </Literal>
        <Literal>
          <ID>thirdParameter</ID>
          <ToolTip>Replace with the name of the third parameter</ToolTip>
          <Default>thirdParameter</Default>
        </Literal>
        <Literal>
          <ID>thirdType</ID>
          <ToolTip>Replace with the name of the third type</ToolTip>
          <Default>thirdType</Default>
        </Literal>
        <Literal>
          <ID>fourthParameter</ID>
          <ToolTip>Replace with the name of the fourth parameter</ToolTip>
          <Default>fourthParameter</Default>
        </Literal>
        <Literal>
          <ID>fourthType</ID>
          <ToolTip>Replace with the name of the fourth type</ToolTip>
          <Default>fourthType</Default>
        </Literal>
      </Declarations>      
      <Code Language="csharp"><![CDATA[private $firstType$ $firstParameter$;
      private $secondType$ $secondParameter$;
      private $thirdType$ $thirdParameter$;
      private $fourthType$ $fourthParameter$;
      
      public $classname$ ($firstType$ $firstParameter$, $secondType$ $secondParameter$, $thirdType$ $thirdParameter$, $fourthType$ $fourthParameter$)
	{
        this.$firstParameter$ = $firstParameter$;
        this.$secondParameter$ = $secondParameter$;
        this.$thirdParameter$ = $thirdParameter$;
        this.$fourthParameter$ = $fourthParameter$;$end$		
	}]]>
			</Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>