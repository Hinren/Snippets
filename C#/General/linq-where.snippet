        <Snippet>
            <Declarations>
                <Literal>
                    <ID>selectedCollection</ID>
                    <ToolTip>Change selected collection name</ToolTip>
                    <Default>selectedCollection</Default>
                </Literal>
                <Literal>
                    <ID>collection</ID>
                    <ToolTip>Write name collection</ToolTip>
                    <Default>collection</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp">

   <![CDATA[var $selectedCollection$ = $collection$
                        .Where(x => x$end$);]]>	
              
            </Code>
          <Imports>
            <Import>
              <Namespace>System.Linq</Namespace>
            </Import>
            <Import>
              <Namespace>System.Collections.Generic</Namespace>
            </Import>
          </Imports>
